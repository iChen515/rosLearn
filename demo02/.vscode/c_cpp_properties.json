{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                //下面这一行是为了导入ros melodic的路径，使得编写调用ros代码时有代码提示并避免误跑异常
                "/opt/ros/melodic/include/**",
                //下面这一行是为了导入生成的C++ 头文件(Person.h)的路径，使得编写调用cpp代码时有代码提示并避免误跑异常
                "/home/user/rosLearn/demo02/devel/include/**",
                //下面这一行是为了导入生成的C++ 头文件(Addints.h等)的路径，使得编写调用cpp代码时有代码提示并避免误跑异常
                "/home/user/rosLearn/demo02/devel/include/**",

                //下面这一行是为了导入自定义的C++ 头文件(hello.h等)的路径，使得编写调用cpp代码时有代码提示并避免误跑异常
                "/home/user/rosLearn/demo02/src/plumbing_head/include/**",
                // "${workspaceFolder}/src/plumbing_head/include/**"
                
                "${workspaceFolder}/src/plumbing_head_src/include/**"

            ],
            "defines": [],
            "intelliSenseMode":"linux-gcc-x64",
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c11",
            "cppStandard": "c++17"
        }
    ],
    "version": 4
}